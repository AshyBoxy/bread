{
  "name": "bread",
  "version": "1.0.0",
  "description": "Discord Bot",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">19.7.0"
  },
  "scripts": {
    "start": "node --experimental-loader ./loader.js dist/index.js",
    "dev": "npm run build ; npm run fix-files ; npm run start",
    "build": "rm -rf dist/ ; tsc",
    "fix-files": "cp -v src/framework/package.json dist/framework ; mv dist/framework/src dist/framework/dist",
    "check": "eslint . ; tsc --noEmit"
  },
  "keywords": [],
  "author": "AshyBoxy",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^1.5.0",
    "discord-api-types": "^0.37.35",
    "discord.js": "^14.7.1",
    "leveldown": "^6.1.1",
    "levelup": "^5.1.1",
    "node-fetch": "^3.3.0",
    "resolve": "^1.22.1"
  },
  "devDependencies": {
    "@types/leveldown": "^4.0.3",
    "@types/levelup": "^5.1.2",
    "@types/node": "^18.14.6",
    "@types/node-fetch": "^2.6.2",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "typescript": "^4.9.5"
  }
}
